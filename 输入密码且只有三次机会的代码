int main()
{
	char arr[20] = { 0 };
	printf("请首次输入密码\n");
	scanf("%s", arr);
	if (strcmp(arr, "233258")==0)
	{
		printf("请再次输入密码\n");
	}
    else
	{
		printf("请再次确定正确密码\n");
		scanf("%s", arr);
		if (strcmp(arr, "233258") == 0)
		{
			printf("输入密码正确\n");
		}
		else
		{
			printf("请再次确定正确密码\n");
			scanf("%s", arr);
			if (strcmp(arr, "233258") == 0)
			{
				printf("输入密码正确\n");
			}
			else
			{
				printf("密码输入错误，请一分钟后再输入");
			}
		}
	}

	return 0;
}
